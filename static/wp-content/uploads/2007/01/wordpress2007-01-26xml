<wp:meta_value>a:7:{i:0;O:8:"stdClass":1:{s:3:"tag";s:7:"Aktüel";}i:1;O:8:"stdClass":1:{s:3:"tag";s:13:"Öykü-Fıkra";}i:2;O:8:"stdClass":1:{s:3:"tag";s:10:"bilgisayar";}i:3;O:8:"stdClass":1:{s:3:"tag";s:8:"cinsiyet";}i:4;O:8:"stdClass":1:{s:3:"tag";s:5:"erkek";}i:5;O:8:"stdClass":1:{s:3:"tag";s:5:"hayat";}i:6;O:8:"stdClass":1:{s:3:"tag";s:6:"kadın";}}</wp:meta_value>
