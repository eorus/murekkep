<wp:status>publish</wp:status>
<wp:post_parent>0</wp:post_parent>
<wp:post_type></wp:post_type>
	
<wp:postmeta>
<wp:meta_key>_utw_tags_0</wp:meta_key>
<wp:meta_value>a:12:{i:0;O:8:"stdClass":1:{s:3:"tag";s:7:"Aktüel";}i:1;O:8:"stdClass":1:{s:3:"tag";s:6:"avatar";}i:2;O:8:"stdClass":1:{s:3:"tag";s:4:"digg";}i:3;O:8:"stdClass":1:{s:3:"tag";s:6:"flickr";}i:4;O:8:"stdClass":1:{s:3:"tag";s:9:"fotoğraf";}i:5;O:8:"stdClass":1:{s:3:"tag";s:8:"internet";}i:6;O:8:"stdClass":1:{s:3:"tag";s:6:"lastfm";}i:7;O:8:"stdClass":1:{s:3:"tag";s:7:"mypictr";}i:8;O:8:"stdClass":1:{s:3:"tag";s:5:"resim";}i:9;O:8:"stdClass":1:{s:3:"tag";s:15:"Site-Önerileri";}i:10;O:8:"stdClass":1:{s:3:"tag";s:6:"upload";}i:11;O:8:"stdClass":1:{s:3:"tag";s:7:"youtube";}}</wp:meta_value>
