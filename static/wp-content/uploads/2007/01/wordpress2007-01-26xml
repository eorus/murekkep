<wp:meta_key>_utw_tags_0</wp:meta_key>
<wp:meta_value>a:10:{i:0;O:8:"stdClass":1:{s:3:"tag";s:6:"defter";}i:1;O:8:"stdClass":1:{s:3:"tag";s:5:"diary";}i:2;O:8:"stdClass":1:{s:3:"tag";s:8:"günlük";}i:3;O:8:"stdClass":1:{s:3:"tag";s:5:"hayat";}i:4;O:8:"stdClass":1:{s:3:"tag";s:10:"moleschino";}i:5;O:8:"stdClass":1:{s:3:"tag";s:9:"moleskine";}i:6;O:8:"stdClass":1:{s:3:"tag";s:12:"moleskinerie";}i:7;O:8:"stdClass":1:{s:3:"tag";s:8:"notebook";}i:8;O:8:"stdClass":1:{s:3:"tag";s:5:"tarih";}i:9;O:8:"stdClass":1:{s:3:"tag";s:8:"van-gogh";}}</wp:meta_value>
